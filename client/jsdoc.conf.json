{
  "tags": {
      "allowUnknownTags": true
  },
  "source": {
      "include": ["./src"],
      "includePattern": ".+\\.js(doc|x)?$"
  },
  "plugins": [
      "plugins/markdown",
      "better-docs/component"
  ],
  "opts": {
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true,
      "template": "./node_modules/better-docs",
      "readme": "./README.md"
  },
  "templates": {
      "better-docs": {
          "name": "PI-Videogames"
      }
  }
}